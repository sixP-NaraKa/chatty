{
    "name": "chatty",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "serve": "ng serve --port 4300",
        "serveall": "ng serve --host 0.0.0.0 --port 4300",
        "sas": "ng serve --host 0.0.0.0 --port 4300 --ssl true --ssl-key chatty-server-key.pem --ssl-cert chatty-server-cert.pem",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "prettier:write": "prettier --write .",
        "prettier:write-cache": "prettier --cache --write ."
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.3.0",
        "@angular/common": "~13.3.0",
        "@angular/compiler": "~13.3.0",
        "@angular/core": "~13.3.0",
        "@angular/forms": "~13.3.0",
        "@angular/platform-browser": "~13.3.0",
        "@angular/platform-browser-dynamic": "~13.3.0",
        "@angular/router": "~13.3.0",
        "ngx-infinite-scroll": "^13.0.0",
        "ngx-socket-io": "^4.2.0",
        "ngx-toastr": "^14.3.0",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.3.7",
        "@angular/cli": "~13.3.7",
        "@angular/compiler-cli": "~13.3.0",
        "@types/jasmine": "~3.10.0",
        "@types/node": "^12.11.1",
        "jasmine-core": "~4.0.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "prettier": "^2.3.2",
        "tailwindcss": "^3.1.3",
        "typescript": "~4.6.2"
    },
    "scarfSettings": {
        "enabled": false
    }
}
