{
    "name": "chatty",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "serve": "ng serve --port 4300",
        "serveall": "ng serve --host 0.0.0.0 --port 4300",
        "sas": "ng serve --host 0.0.0.0 --port 4300 --ssl true --ssl-key chatty-server-key.pem --ssl-cert chatty-server-cert.pem",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "jest",
        "test-watch": "jest --only-changed",
        "test-watch-log": "jest --only-changed --silent=false",
        "prettier:write": "prettier --write .",
        "prettier:write-cache": "prettier --cache --write ."
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^16.2.12",
        "@angular/common": "^16.2.12",
        "@angular/compiler": "^16.2.12",
        "@angular/core": "^16.2.12",
        "@angular/forms": "^16.2.12",
        "@angular/platform-browser": "^16.2.12",
        "@angular/platform-browser-dynamic": "^16.2.12",
        "@angular/router": "^16.2.12",
        "ngx-infinite-scroll": "^16.0.0",
        "ngx-socket-io": "4.5.0",
        "ngx-toastr": "^17.0.2",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.13.0"
    },
    "devDependencies": {
        "@angular-builders/jest": "^16.0.1",
        "@angular-devkit/build-angular": "^16.2.14",
        "@angular/cli": "^16.2.14",
        "@angular/compiler-cli": "^16.2.12",
        "@types/jest": "^29.5.12",
        "@types/node": "^12.11.1",
        "glob": "^10.3.10",
        "jest": "^29.7.0",
        "jest-preset-angular": "^14.0.3",
        "ng-mocks": "^14.12.1",
        "prettier": "^2.3.2",
        "tailwindcss": "^3.1.3",
        "ts-jest": "^29.1.2",
        "typescript": "~4.9.3"
    },
    "scarfSettings": {
        "enabled": false
    }
}
