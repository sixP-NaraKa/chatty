{
    "name": "chatty",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "serve": "ng serve --port 4300",
        "serveall": "ng serve --host 0.0.0.0 --port 4300",
        "sas": "ng serve --host 0.0.0.0 --port 4300 --ssl true --ssl-key chatty-server-key.pem --ssl-cert chatty-server-cert.pem",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "test-watch": "ng test --only-changed",
        "test-watch-log": "ng test --only-changed --silent=false",
        "prettier:write": "prettier --write .",
        "prettier:write-cache": "prettier --cache --write ."
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.3.0",
        "@angular/common": "~13.3.0",
        "@angular/compiler": "~13.3.0",
        "@angular/core": "~13.3.0",
        "@angular/forms": "~13.3.0",
        "@angular/platform-browser": "~13.3.0",
        "@angular/platform-browser-dynamic": "~13.3.0",
        "@angular/router": "~13.3.0",
        "ngx-infinite-scroll": "^13.0.0",
        "ngx-socket-io": "^4.2.0",
        "ngx-toastr": "^14.3.0",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-builders/jest": "^12.1.2",
        "@angular-devkit/build-angular": "~13.3.7",
        "@angular/cli": "~13.3.7",
        "@angular/compiler-cli": "~13.3.0",
        "@types/jest": "^27.0.3",
        "@types/node": "^12.11.1",
        "glob": "^10.3.10",
        "jest": "^27.0.3",
        "jest-preset-angular": "^11.0.1",
        "ng-mocks": "^14.12.1",
        "prettier": "^2.3.2",
        "tailwindcss": "^3.1.3",
        "ts-jest": "27.0.3",
        "typescript": "~4.6.2"
    },
    "scarfSettings": {
        "enabled": false
    }
}
